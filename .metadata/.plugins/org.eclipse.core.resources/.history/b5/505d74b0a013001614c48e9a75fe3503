						<div class="profile_details_left">
							<ul class="nofitications-dropdown">
								<li class="dropdown note dra-down">
									<script type="text/javascript">
										function DropDown(el) {
											this.dd = el;
											this.placeholder = this.dd
													.children('span');
											this.opts = this.dd
													.find('ul.dropdown > li');
											this.val = '';
											this.index = -1;
											this.initEvents();
										}
										DropDown.prototype = {
											initEvents : function() {
												var obj = this;

												obj.dd.on('click', function(
														event) {
													$(this).toggleClass(
															'active');
													return false;
												});

												obj.opts
														.on(
																'click',
																function() {
																	var opt = $(this);
																	obj.val = opt
																			.text();
																	obj.index = opt
																			.index();
																	obj.placeholder
																			.text(obj.val);
																});
											},
											getValue : function() {
												return this.val;
											},
											getIndex : function() {
												return this.index;
											}
										}

										$(function() {

											var dd = new DropDown($('#dd'));

											$(document)
													.click(
															function() {
																// all dropdowns
																$(
																		'.wrapper-dropdown-3')
																		.removeClass(
																				'active');
															});

										});
									</script>
								</li>

							</ul>
						</div>
						<!--//profile_details-->
					</div>
					<!--//menu-right-->
				</div>
